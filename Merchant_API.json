                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "phone_number": {
                        "type": "string"
                    },
                    "pin_code": {
                        "type": "string"
                    },
                    "address": {
                        "type": "string"
                    },
                    "city": {
                        "type": "string"
                    },
                    "state": {
                        "type": "string"
                    },
                    "latitude": {
                        "type": "number"
                    },
                    "longitude": {
                        "type": "number"
                    },
                    "business_name": {
                        "type": "string"
                    },
                    "business_mobile_number": {
                        "type": "string",
                        "nullable": true
                    },
                    "gst": {
                        "type": "string"
                    },
                    "business_type": {
                        "type": "string"
                    },
                    "business_category": {
                        "type": "string"
                    },
                    "business_email_id": {
                        "type": "string"
                    }
                }
            },
            "MerchantsResponse": {
                "type": "object",
                "properties": {
                    "merchants": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Merchant"
                        }
                    }
                }
            }
        }
    }
}