{
    "openapi": "3.0.0",
    "info": {
        "title": "Merchants API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://34.8.142.182.nip.io/merchants"
        }
    ],
    "paths": {
        "/": {
            "get": {
                "responses": {
                    "200": {
